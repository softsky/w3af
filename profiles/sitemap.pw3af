[profile]
description = Use different online techniques to create a fast sitemap of the target web application. This plugin will only work if you've got Internet access and the target web application is being spidered by Yahoo!
name = sitemap

[crawl.sitemap_xml]

[misc-settings]
fuzzed_files_extension = gif
fuzzable_headers = 
fuzz_form_files = True
fuzz_url_filenames = False
max_discovery_time = 120
form_fuzzing_mode = tmb
non_targets = 
fuzz_url_parts = False
stop_on_first_exception = False
interface = ppp0
local_ip_address = 10.5.6.18
msf_location = /opt/metasploit3/bin/
params_max_variants = 10
path_max_variants = 50

[http-settings]
proxy_port = 8080
url_parameter = 
never_404 = 
headers_file = 
proxy_address = 
basic_auth_domain = 
always_404 = 
max_http_retries = 2
ntlm_auth_user = 
ntlm_auth_passwd = 
ignore_session_cookies = False
timeout = 30
user_agent = CloudScan/1.1
basic_auth_user = 
basic_auth_passwd = 
string_match_404 = 
cookie_jar_file = 
ntlm_auth_domain = 
ntlm_auth_url = 
webkit_address = http://127.0.0.1:25555

[output.text_file]
verbose = True
output_file = /var/log/cloudscan/w3af_runtime.log
http_output_file = /var/log/cloudscan/w3af_runtime_http.log

[crawl.sitemap_xml]

[crawl.bing_spider]

[crawl.robots_txt]

[crawl.archive_dot_org]

[crawl.noah_spider]
only_forward = False
follow_regex = .*
ignore_regex =
pipe_address =
max_depth = 0
max_urls = 0
use_webkit = False

[crawl.urllist_txt]

[crawl.google_spider]

